{
  "name": "bolt-express",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "devv": "yarn ccweb-add-on-scripts start --use \"vite build\"",
    "build": "vite build",
