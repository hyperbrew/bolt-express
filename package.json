    "@tsconfig/svelte": "^5.0.4",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "serve": "^14.2.3",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.5",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.4.1"
  }
}
